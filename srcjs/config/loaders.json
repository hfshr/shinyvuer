[
  {
    "test": "\\.(js|jsx)$",
    "use": [
      "babel-loader"
    ],
    "exclude": "/node_modules/"
  },
  {
    "test": "\\.vue$",
    "use": [
      "vue-loader"
    ],
    "exclude": "/node_modules/"
  },
  {
    "test": "\\.css$",
    "use": [
      "vue-style-loader",
      {
        "loader": "css-loader",
        "options": {
          "esModule": false
        }
      }
    ],
    "exclude": "/node_modules/"
  },
  {
    "test": "\\.(png|jpe?g|gif)$/i",
    "use": [
      "file-loader"
    ]
  }
]
